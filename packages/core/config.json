{
  "$schema": "./server/config-schema.json",
  "loggingLevel": "verbose",
  "theme": {
    "pages": {
      "Home": {
        "HomeContainer": {
          "maxWidth": 1500
        }
      }
    }
  },
  "connectors": [
    {
      "type": "DOCKER",
      "config": {
        "containerMap": {
          "plugsy": {
            "category": "Home",
            "icon": "@styled-icons/boxicons-regular/Crown",
            "name": "Plugsy"
          }
        }
      }
    },
    {
      "type": "WEBSITE",
      "config": {
        "sites": [
          {
            "request": "https://google.com",
            "display": {
              "name": "Home Assistant",
              "category": "Home",
              "icon": "simple-icons/plex",
              "link": "https://my.homeassistant.com"
            }
          },
          {
            "request": "https://localhost:1294",
            "display": {
              "name": "Unifi",
              "category": "Home",
              "icon": "boxicons-regular/network-chart",
              "link": "https://my.unifi.com"
            }
          },
          {
            "request": "https://localhost:9345",
            "display": {
              "name": "Unraid",
              "category": "Home",
              "icon": "fa-solid/server",
              "link": "https://my.unraid.com"
            }
          }
        ]
      }
    }
  ]
}
