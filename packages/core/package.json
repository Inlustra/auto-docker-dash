{
  "name": "@plugsy/core",
  "version": "4.0.0",
  "license": "MIT",
  "config": {
    "imageRepo": "plugsy/core"
  },
  "engines": {
    "node": "14"
  },
  "scripts": {
    "start": "DOCKER_DASH_LOCAL_CONFIG_FILE=./config.json nodemon",
    "get-version": "node -pe \"require('./package.json').version\"",
    "type-check": "tsc",
    "web:prod": "next start",
    "analyze": "serve .next/analyze",
    "gen:gql": "graphql-codegen",
    "gen:json": "typescript-json-schema ./tsconfig.gen.json ServerConfig --aliasRefs --ignoreErrors --required --strictNullChecks -o ./server/config-schema.json && yarn node fix-config-json.js ./server/config-schema.json",
    "gen": "yarn gen:gql && yarn gen:json",
    "build": "yarn gen && tsc --project tsconfig.server.json && ANALYZE=TRUE next build",
    "dev:build": "tsc --project tsconfig.server.json",
    "dev": "yarn gen && concurrently --kill-others-on-fail -n  gql,tsc,node \"yarn gen:gql --watch\" \"yarn dev:build -w\" \"yarn start\"",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@babel/runtime": "^7.14.8",
    "@graphql-tools/schema": "^7.1.5",
