{
  "name": "@plugsy/core",
  "version": "4.0.0",
  "license": "MIT",
  "config": {
    "imageRepo": "plugsy/core"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "./server/config-schema.json",
              "label": "Config Schema"
            }
          ]
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ]
    ]
  },
  "scripts": {
    "start": "DOCKER_DASH_LOCAL_CONFIG_FILE=./config.json nodemon",
    "get-version": "node -pe \"require('./package.json').version\"",
    "release": "semantic-release",
    "type-check": "tsc",
    "web:prod": "next start",
    "analyze": "serve .next/analyze",
    "gen:json": "typescript-json-schema ./tsconfig.json ServerConfig --aliasRefs --ignoreErrors --required --strictNullChecks -o ./server/config-schema.json && yarn node fix-config-json.js ./server/config-schema.json",
    "gen:icons": "node ./icongenerator.js",
    "gen": "yarn gen:icons && yarn gen:json",
    "build": "yarn gen && parcel build ./server/index.ts --target node && ANALYZE=TRUE next build",
    "dev:build": "parcel ./server/index.ts --target node",
    "dev": "yarn gen && concurrently --kill-others-on-fail -n  gql,parcel,node  \"yarn dev:build\" \"yarn start\"",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@babel/runtime": "^7.14.6",
    "@graphql-tools/schema": "^7.1.5",
    "@plugsy/agent": "workspace:packages/agent",
    "@plugsy/common": "workspace:packages/common",
    "@plugsy/connectors": "workspace:packages/connectors",
    "@popperjs/core": "^2.9.2",
    "@react-icons/all-files": "^4.1.0",
    "apollo-link-logger": "^2.0.0",
    "apollo-server-express": "^2.25.2",
    "apollo-server-micro": "^2.25.2",
    "axios": "^0.21.1",
    "babel-types": "^6.26.0",
    "dataloader": "^2.0.0",
    "date-fns": "^2.22.1",
    "deepmerge": "^4.2.2",
    "dockerode": "^3.3.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "graphql-scalars": "^1.10.0",
    "graphql-tag": "^2.12.5",
    "jsonschema": "^1.4.0",
    "nanoid": "^3.1.23",
    "next": "^11.0.1",
    "raw-loader": "^4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "react-tiny-popover": "^6.0.5",
    "react-use": "^17.2.4",
    "rxjs": "^7.2.0",
    "rxjs-for-await": "^0.0.2",
    "styled-components": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.19",
    "webpack": "^5.43.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@next/bundle-analyzer": "^11.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/dockerode": "^3.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/react": "17.0.13",
    "@types/styled-components": "^5.1.11",
    "babel-plugin-styled-components": "^1.13.1",
    "concurrently": "^6.2.0",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "cross-conf-env": "^1.2.1",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "jest": "^27.0.6",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-images": "^1.8.1",
    "next-transpile-modules": "^8.0.0",
    "nodemon": "^2.0.9",
    "parcel-bundler": "^1.12.5",
    "rxjs-marbles": "^7.0.0",
    "semantic-release": "^17.4.4",
    "ts-node": "^10.0.0",
    "typescript": "4.3.5",
    "typescript-json-schema": "^0.50.1"
  }
}
