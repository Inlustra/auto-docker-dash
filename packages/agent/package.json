{
  "name": "@plugsy/agent",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "concurrently --kill-others-on-fail -n tsc,node 'tsc -b --watch' 'nodemon'",
    "build:lib": "tsc --build",
    "build": "yarn gen && yarn build:lib",
    "gen:json": "typescript-json-schema ./tsconfig.json Config --aliasRefs --ignoreErrors --required --strictNullChecks -o ./src/config-schema.json",
    "gen": "yarn gen:json"
  },
  "dependencies": {
    "@plugsy/common": "workspace:packages/common",
    "@plugsy/connectors": "workspace:packages/connectors",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.5",
    "rxjs": "^7.2.0",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": "14"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12",
    "typescript": "^4.3.5",
    "typescript-json-schema": "^0.50.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
