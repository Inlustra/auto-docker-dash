    "@graphql-tools/stitch": "^8.4.3",
    "@plugsy/common": "workspace:packages/common",
    "child-process-promise": "^2.2.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.2",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "graphql-scalars": "^1.13.6",
    "graphql-tag": "^2.12.6",
    "nanoid": "^3.1.30",
