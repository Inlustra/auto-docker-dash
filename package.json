{
  "name": "auto-docker-dash",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "config": {
    "imageRepo": "inlustra/autodockerdash"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ]
    ]
  },
  "scripts": {
    "start": "nodemon",
    "get-version": "node -pe \"require('./package.json').version\"",
    "release": "semantic-release",
    "type-check": "tsc",
    "build": "yarn icongen && yarn codegen && parcel build ./server/index.ts --target node && ANALYZE=TRUE next build",
    "web:prod": "next start",
    "analyze": "serve .next/analyze",
    "codegen": "graphql-codegen",
    "icongen": "node icongenerator.js",
    "dev:build": "parcel ./server/index.ts --target node",
    "dev": "yarn icongen && yarn codegen && concurrently --kill-others-on-fail -n  gql,parcel,node \"yarn codegen -w\" \"yarn dev:build\" \"yarn start\""
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@graphql-tools/schema": "^7.1.5",
    "@popperjs/core": "^2.9.2",
    "@react-icons/all-files": "^4.1.0",
    "apollo-link-logger": "^2.0.0",
    "apollo-server-express": "^2.25.1",
    "apollo-server-micro": "^2.25.1",
    "dataloader": "^2.0.0",
    "date-fns": "^2.22.1",
    "dockerode": "^3.3.0",
    "graphql": "^15.5.0",
    "next": ">=9.0.0",
    "next-transpile-modules": "^7.3.0",
    "raw-loader": "^4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fitty": "^1.0.1",
    "react-tiny-popover": "^6.0.5",
    "react-use": "^17.2.4",
    "rxjs": "^7.1.0",
    "rxjs-for-await": "^0.0.2",
    "styled-components": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "resolutions": {
    "graphql": "15.5.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/import-types-preset": "^1.18.1",
    "@graphql-codegen/named-operations-object": "^1.17.9",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typed-document-node": "^1.18.2",
    "@graphql-codegen/typescript-document-nodes": "^1.17.9",
    "@graphql-codegen/typescript-graphql-request": "3.2.1",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@next/bundle-analyzer": "^10.2.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/dockerode": "^3.2.3",
    "@types/react": "17.0.11",
    "@types/styled-components": "^5.1.10",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.2.0",
    "cross-conf-env": "^1.2.1",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-images": "^1.7.0",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.5",
    "semantic-release": "^17.4.4",
    "ts-node": "^10.0.0",
    "typescript": "4.3.2"
  }
}
